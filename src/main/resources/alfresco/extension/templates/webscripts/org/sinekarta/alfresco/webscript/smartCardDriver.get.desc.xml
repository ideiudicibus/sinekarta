<webscript>
  <shortname>sinekarta smart card driver supported</shortname>
  <description>
	script to get a single or a list of smart card driver supported.
	accept one optional parameters : 
	driver - to query for a specified driver
	if no parameter is given, this service will return the complete list of all smart card driver supported
	supported return types : xml, json 
	default : xml
  </description>
  <url>/sinekarta/smartCardDriver?driver={driver?}</url>
  <format default="xml">any</format>
  <authentication>user</authentication>
  <transaction>required</transaction>
</webscript>
