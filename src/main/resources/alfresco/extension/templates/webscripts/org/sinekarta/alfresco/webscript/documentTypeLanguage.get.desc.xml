<webscript>
  <shortname>sinekarta documentType supported languages</shortname>
  <description>
	script to get a single or a list of document type supported languages.
	accept one optional parameters : 
	languageCode - to query for a specified language
	if no parameter is given, this service will return the complete list of all document type languages
	supported return types : xml, json 
	default : xml
  </description>
  <url>/sinekarta/documentTypeLanguage?languageCode={languageCode?}</url>
  <format default="xml">any</format>
  <authentication>user</authentication>
  <transaction>required</transaction>
</webscript>
